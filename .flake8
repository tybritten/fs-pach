[flake8]
per-file-ignores = __init__.py:F401
ignore = E203, E266, E501, W503
max-line-length = 88
max-complexity = 18
select = B,C,E,F,W,T4,B9
exclude =
    __pycache__,
    .cache,
    .circleci,
    .direnv
    .git,
    .pytest_cache,
    .serverless
    .tox,
    .vscode,
    html5cov,
    htmlcov,
    node_modules,
    output,
    venv,
    .venv,
    ldist,
    cdk.out,
    .build
